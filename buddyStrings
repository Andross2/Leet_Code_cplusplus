#include <iostream>
#include <string>

class Solution {
public:
    bool buddyStrings(string A, string B) {
    
    if(A.length() != B.length()) 
    {      
     return false;   
    }
    
    if(A == B) 
    {
        int *count = new int[26];
        for (int i = 0; i < A.length(); ++i) 
        {
            count[A.at(i)]
        }
        
    }
        
    }
};
